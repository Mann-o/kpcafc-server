{
  "name": "kpcafc-server",
  "version": "0.0.1",
  "description": "API server for KPCAFC Portal",
  "main": "server.js",
  "scripts": {
    "dev": "adonis serve --dev",
    "dev:debug": "adonis serve --dev --debug",
    "start": "cross-env NODE_ENV=production node server.js",
    "migrate:dev": "adonis migration:refresh && adonis seed",
    "migrate:production": "adonis migration:run --force",
    "test": "adonis test"
  },
  "author": "Liam Potter <me@liam-potter.co.uk>",
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.9",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.13",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/redis": "^2.0.7",
    "@adonisjs/validator": "^5.0.6",
    "adonis-acl": "^1.1.1",
    "adonis-cache": "^0.3.4",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "cross-env": "^6.0.3",
    "date-fns": "^2.5.1",
    "lodash": "^4.17.15",
    "pg": "^7.12.1",
    "uuid": "^3.3.3",
    "uuid-validate": "^0.0.3"
  },
  "devDependencies": {
    "faker": "^4.1.0"
  },
  "resolutions": {
    "knex": "0.19.5"
  },
  "autoload": {
    "App": "./app"
  }
}
